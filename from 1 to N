#include <bits/stdc++.h>

using namespace std;

string toString(int n){
	string t ="";
	while(n > 0){
		t += char( n % 10 + '0');
		n /= 10;
	}
	reverse(t.begin(), t.end());
	return t;
}

string num(int n){
	if(n == 1) return "1";
	return num(n - 1) + " " + toString(n);
}

int main(){
	int n;
	cin >> n;
	cout << num(n);
}
